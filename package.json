{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run clean && prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "prisma:generate": "prisma generate",
    "prisma:seed": "npx ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts",
    "prisma:deploy": "prisma migrate deploy",
    "postinstall": "prisma generate",
    "db:push": "prisma db push --accept-data-loss",
    "db:migrate": "node scripts/apply-migrations.js",
    "db:migrate:force": "node scripts/apply-migrations.js --force",
    "db:seed": "node scripts/apply-migrations.js --seed"
  },
  "dependencies": {
    "@prisma/client": "^6.8.1",
    "bcrypt": "^6.0.0",
    "next": "15.2.4",
    "prisma": "^6.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^22",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
